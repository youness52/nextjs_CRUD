generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  age   Int
  image String? // Stores filename or URL to the uploaded image
}

model Teacher {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  special String
}

model User {
  id       Int    @id @default(autoincrement())
  name     String?
  email    String @unique
  password String
}
